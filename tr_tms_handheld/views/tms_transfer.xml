<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="tms_transfer_header_view_tree" model="ir.ui.view">
        <field name="name">tms.transfer.header.view.tree</field>
        <field name="model">tms.transfer.header</field>
        <field name="arch" type="xml">
            <tree string="TMS Transfer Header">
                <field name="no" />
                <field name="transfer_from_code" />
                <field name="transfer_to_code" />
                <field name="in_transit_code" />
                <field name="posting_date" />
                <field name="status" />
            </tree>
        </field>
    </record>

    <record id="tms_transfer_header_view_form" model="ir.ui.view">
        <field name="name">tms.transfer.header.view.form</field>
        <field name="model">tms.transfer.header</field>
        <field name="arch" type="xml">
            <form string="TMS Transfer Header">
                <sheet>
                    <group string="Transfer Information" colspan="2">
                        <group colspan="1">
                            <field name="no" />
                            <field name="status" />
                        </group>
                        <group colspan="1">
                            <field name="posting_date" />
                            <field name="shipment_date" />
                            <field name="receipt_date" />
                        </group>
                    </group>

                    <group string="Transfer From" colspan="2">
                        <group colspan="1">
                            <field name="transfer_from_code" />
                            <field name="transfer_from_name" />
                            <field name="transfer_from_name_2" />
                            <field name="transfer_from_address" />
                        </group>
                        <group colspan="1">
                            <field name="transfer_from_address_2" />
                            <field name="transfer_from_post_code" />
                            <field name="transfer_from_city" />
                            <field name="transfer_from_county" />
                            <field name="transfer_from_country_region_code" />
                        </group>
                    </group>

                    <group string="Transfer To" colspan="2">
                        <group colspan="1">
                            <field name="transfer_to_code" />
                            <field name="transfer_to_name" />
                            <field name="transfer_to_name_2" />
                            <field name="transfer_to_address" />
                        </group>
                        <group colspan="1">
                            <field name="transfer_to_address_2" />
                            <field name="transfer_to_post_code" />
                            <field name="transfer_to_city" />
                            <field name="transfer_to_county" />
                            <field name="transfer_to_country_region_code" />
                        </group>
                    </group>

                    <group string="Additional Information" colspan="2">
                        <group colspan="1">
                            <field name="in_transit_code" />
                            <field name="external_document_no" />
                            <field name="completely_shipped" />
                            <field name="completely_received" />
                            <field name="assigned_user_id" />
                            <field name="keterangan" />
                            <field name="supplier_no_packing_list" />
                        </group>
                        <group colspan="1">
                            <field name="customer_no" />
                            <field name="salesperson_code" />
                            <field name="location_sales_type" />
                            <field name="shipment_no_series" />
                            <field name="store_to" />
                            <field name="transfer_type" />
                        </group>
                    </group>

                    <notebook>
                        <page name="po_line" string="Lines">
                            <field name="transfer_line_ids">
                                <tree editable="bottom">
                                    <field name="document_no" force_save="1" invisible="1" />
                                    <field name="line_no" />
                                    <field name="item_no" />
                                    <field name="description" />
                                    <field name="quantity" />
                                    <field name="uom" />
                                    <field name="qty_to_ship" />
                                    <field name="qty_shipped" />
                                    <field name="qty_to_receive" />
                                    <field name="qty_received" />
                                    <field name="status" />
                                    <field name="in_transit_code" />
                                    <field name="shipment_date" />
                                    <field name="variant_code" />
                                    <field name="keterangan_dus" />
                                </tree>
                            </field>
                        </page>
                    </notebook>
                </sheet>
            </form>
        </field>
    </record>

</odoo>