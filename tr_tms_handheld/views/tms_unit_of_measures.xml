<odoo>
  <data>
    <!-- View for TMS Unit of Measures -->
    <record id="view_tms_unit_of_measures_form" model="ir.ui.view">
      <field name="name">tms.unit.of.measures.form</field>
      <field name="model">tms.unit.of.measures</field>
      <field name="arch" type="xml">
        <form string="TMS Unit of Measures">
          <sheet>
            <group>
              <field name="code"/>
              <field name="description"/>
            </group>
          </sheet>
        </form>
      </field>
    </record>
  </data>
</odoo>